<?xml version="1.0" encoding="utf-8"?>
<com.zing.zalo.p077ui.chat.chatrow.ChatRowWebContentPlus xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/chat_left_rich_message_mc_row"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="@dimen/f1s"
        android:textColor="?attr/LinkColor"
        android:layout_gravity="center"
        android:id="@+id/btn_seemore"
        android:background="@drawable/chat_msg_info_background"
        android:paddingLeft="12dp"
        android:paddingTop="8dp"
        android:paddingRight="12dp"
        android:paddingBottom="8dp"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_see_more_msg_info_group"
        android:includeFontPadding="false"/>
    <RelativeLayout
        android:id="@+id/content_container_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.zing.zalo.p077ui.custom.PhotoToggleButton
            android:id="@+id/select_checkbox"
            android:visibility="gone"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginLeft="8dp"
            app:checkedImage="@drawable/icn_csc_select_checked"
            app:uncheckedImage="@drawable/icn_csc_select_unchecked"/>
        <ViewStub
            android:id="@+id/vs_avt_view"
            android:layout="@layout/chat_row_avt_view"
            android:inflatedId="@+id/vs_avt_view"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_toRightOf="@+id/select_checkbox"
            android:layout_marginStart="8.5dp"
            android:layout_marginEnd="2.5dp"/>
        <ViewStub
            android:id="@+id/vs_key_view"
            android:layout="@layout/chat_row_key_view"
            android:inflatedId="@+id/vs_key_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="27dp"/>
        <ViewStub
            android:id="@+id/vs_sender_name"
            android:layout="@layout/chat_row_sender_name_view"
            android:inflatedId="@+id/vs_sender_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/vs_avt_view"
            android:layout_alignBottom="@+id/vs_avt_view"
            android:layout_toEndOf="@+id/vs_avt_view"/>
        <com.zing.zalo.zinstant.ZaloZinstantRootLayout
            android:id="@+id/zinstant_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/vs_sender_name"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/zinstant_layout"
            android:layout_alignBottom="@+id/zinstant_layout"
            android:layout_toStartOf="@+id/zinstant_layout">
            <ImageButton
                android:id="@+id/btn_retry_sending"
                android:background="@color/transparent"
                android:padding="6dp"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_retry_line_circle_24"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/layout_state"
            android:background="@drawable/white_rounded_radius_100_bg"
            android:paddingTop="3dp"
            android:paddingBottom="3dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="6dp"
            android:layout_below="@+id/zinstant_layout"
            android:layout_alignRight="@+id/zinstant_layout"
            android:backgroundTint="?attr/ChatStateBackgroundColor1">
            <androidx.appcompat.widget.ZAppCompatImageView
                android:id="@+id/icon_state"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="4dp"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="11sp"
                android:textColor="?attr/ChatStateTextColor"
                android:id="@+id/text_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="6dp"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/layout_time"
            android:background="@drawable/white_rounded_radius_100_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/zinstant_layout"
            android:layout_alignStart="@+id/zinstant_layout"
            android:backgroundTint="?attr/ChatStateBackgroundColor1">
            <androidx.appcompat.widget.ZAppCompatImageView
                android:id="@+id/icon_ttl"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3dp"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="11sp"
                android:textColor="?attr/ChatStateTextColor"
                android:layout_gravity="center_vertical"
                android:id="@+id/text_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="6dp"/>
        </LinearLayout>
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textSize="11sp"
            android:textColor="?attr/ChatStateTextColor"
            android:id="@+id/count_down_time"
            android:background="@drawable/white_rounded_radius_100_bg"
            android:paddingLeft="6dp"
            android:paddingRight="6dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_below="@+id/layout_time"
            android:layout_alignStart="@+id/zinstant_layout"
            android:backgroundTint="?attr/ChatStateBackgroundColor1"/>
    </RelativeLayout>
    <View
        android:id="@+id/chat_row_webcontent_margin_bottom"
        android:layout_width="1dp"
        android:layout_height="2dp"/>
</com.zing.zalo.p077ui.chat.chatrow.ChatRowWebContentPlus>
