<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/layout_friend_suggest_profile"
    android:background="?attr/ProfilePrimaryBackgroundColor"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:id="@+id/suggest_friend_separate"
        android:background="?attr/SecondaryBackgroundColor"
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <com.zing.zalo.feed.components.FeedItemSuggestFriends
        android:orientation="vertical"
        android:id="@+id/suggest_friend_pager"
        android:background="?attr/ProfilePrimaryBackgroundColor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
