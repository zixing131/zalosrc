<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fstyle="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:background="?attr/PrimaryBackgroundColor"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:gravity="center_horizontal"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.ZAppCompatImageView
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fstyle:srcCompatEx="0x7f080fc7"/>
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textSize="22dp"
            android:textColor="?attr/HeaderFormTitleColor"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="50dp"
            android:text="@string/str_title_operate_enable_submit_contact"
            style="@style/Roboto.f1"/>
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textSize="@dimen/f2"
            android:textColor="?attr/TextColor1"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="32dp"
            android:layout_marginTop="14dp"
            android:layout_marginRight="32dp"
            android:text="@string/str_content_operate_enable_submit_contact"/>
        <com.zing.zalo.p077ui.widget.RobotoButton
            android:textColor="@drawable/stencils_button_style3_text"
            android:layout_gravity="center_horizontal"
            android:id="@+id/btnEnable"
            android:layout_width="match_parent"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="30dp"
            android:layout_marginRight="45dp"
            android:text="@string/str_btn_enable_submit_contact"
            android:textAllCaps="true"
            style="@style/btnType1.medium"/>
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textSize="@dimen/f5"
            android:textColor="?attr/HeaderFormTitleColor"
            android:gravity="center"
            android:id="@+id/tvSkip"
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:layout_marginBottom="36dp"
            android:text="@string/btn_skip"/>
    </LinearLayout>
</LinearLayout>
