<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <FrameLayout
        android:id="@+id/layoutFriendSuggest"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textSize="@dimen/f13"
            android:textColor="?attr/SectionTitleColor2"
            android:paddingLeft="@dimen/msg_item_padding_l_r"
            android:paddingTop="@dimen/text_header_list_to_line_margin_t"
            android:paddingBottom="@dimen/label_header_margin_t_b"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/str_native_list_entry_view_title"
            app:fstyle="medium"/>
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textColor="?attr/LinkColor"
            android:layout_gravity="right"
            android:id="@+id/tvSkip"
            android:paddingTop="@dimen/text_header_list_to_line_margin_t"
            android:paddingRight="@dimen/msg_item_padding_l_r"
            android:paddingBottom="@dimen/label_header_margin_t_b"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/str_menu_item_ignore"
            style="@style/Roboto.f13b"/>
    </FrameLayout>
    <RelativeLayout
        android:id="@+id/fLayoutfriendsuggest"
        android:background="@drawable/stencils_contact_bg"
        android:paddingBottom="@dimen/padding_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/layoutNotifyNumber"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/avt_image_size_default"
            android:layout_marginRight="@dimen/msg_item_padding_l_r"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true">
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:layout_gravity="center"
                android:id="@+id/numnotification_layoutfriendsuggest"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/hearder_func_margin_notify_number_ic_next_r"
                style="@style/stencils_notification_text_view"/>
            <ImageView
                android:layout_gravity="center"
                android:id="@+id/icon_next"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="0x7f081044"/>
        </LinearLayout>
        <com.zing.zalo.p077ui.widget.AvatarImageView
            android:id="@+id/img_avt_suggest1"
            android:layout_width="@dimen/avt_image_size_default"
            android:layout_height="@dimen/avt_image_size_default"
            android:layout_marginLeft="@dimen/msg_item_padding_l_r"
            android:layout_marginTop="@dimen/padding_item"
            app:srcCompat="?attr/default_avatar"
            style="@style/myImageView"/>
        <com.zing.zalo.p077ui.widget.AvatarImageView
            android:id="@+id/img_avt_suggest2"
            android:visibility="gone"
            android:layout_width="@dimen/avt_image_size_default"
            android:layout_height="@dimen/avt_image_size_default"
            android:layout_marginLeft="@dimen/hearder_func_avt_suggest_margin_l"
            android:layout_marginTop="@dimen/padding_item"
            android:layout_toRightOf="@+id/img_avt_suggest1"
            app:srcCompat="?attr/default_avatar"
            style="@style/myImageView"/>
        <com.zing.zalo.p077ui.widget.AvatarImageView
            android:id="@+id/img_avt_suggest3"
            android:visibility="gone"
            android:layout_width="@dimen/avt_image_size_default"
            android:layout_height="@dimen/avt_image_size_default"
            android:layout_marginLeft="@dimen/hearder_func_avt_suggest_margin_l"
            android:layout_marginTop="@dimen/padding_item"
            android:layout_toRightOf="@+id/img_avt_suggest2"
            app:srcCompat="?attr/default_avatar"
            style="@style/myImageView"/>
        <com.zing.zalo.p077ui.widget.AvatarImageView
            android:id="@+id/img_avt_suggest4"
            android:visibility="gone"
            android:layout_width="@dimen/avt_image_size_default"
            android:layout_height="@dimen/avt_image_size_default"
            android:layout_marginLeft="@dimen/hearder_func_avt_suggest_margin_l"
            android:layout_marginTop="@dimen/padding_item"
            android:layout_toRightOf="@+id/img_avt_suggest3"
            app:srcCompat="?attr/default_avatar"
            style="@style/myImageView"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:id="@+id/layoutFriendsuggestTitleCenter"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/avt_image_size_default"
            android:layout_marginLeft="@dimen/msg_item_padding_l_r"
            android:layout_marginTop="@dimen/padding_item"
            android:layout_toLeftOf="@+id/layoutNotifyNumber"
            android:layout_toRightOf="@+id/img_avt_suggest4">
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f2"
                android:textColor="?attr/TextColor1"
                android:gravity="center_vertical"
                android:id="@+id/tvLayoutfriendsuggestTitleCenter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f13"
                android:textStyle="normal"
                android:textColor="?attr/TextColor2"
                android:gravity="center_vertical"
                android:id="@+id/tvLayoutfriendsuggestMsgCenter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"/>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
