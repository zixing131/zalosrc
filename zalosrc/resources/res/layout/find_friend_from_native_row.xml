<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:background="?attr/page_background_01"
    android:paddingTop="24dp"
    android:paddingBottom="24dp"
    android:focusable="false"
    android:clickable="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textColor="?attr/text_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:text="@string/str_contact_native_msg_find_more_desc"
        style="@style/t_small"/>
    <com.zing.zalo.zdesign.component.Button
        android:id="@+id/btn_find_friend_native"
        android:layout_width="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/str_contact_native_msg_find_more_action"
        style="@style/ButtonMedium.Primary"/>
</LinearLayout>
