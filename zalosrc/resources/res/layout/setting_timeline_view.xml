<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/ll_swipeable_view"
    android:background="?attr/SecondaryBackgroundColor"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_action_bar"/>
    <ScrollView
        android:id="@+id/scroll_main_layout"
        android:background="?attr/PrimaryBackgroundColor"
        android:scrollbars="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f5"
                android:textColor="?attr/TextColor1"
                android:gravity="center"
                android:background="?attr/SecondaryBackgroundColor"
                android:padding="16dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/str_hint_setting_timeline"
                android:includeFontPadding="false"/>
            <View
                android:background="?attr/ItemSeparatorColor"
                android:layout_width="match_parent"
                android:layout_height="1px"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/ll_select_all"
                android:background="@drawable/stencils_contact_bg"
                android:paddingLeft="@dimen/msg_item_padding_l_r"
                android:paddingTop="8dp"
                android:paddingRight="@dimen/msg_item_padding_l_r"
                android:paddingBottom="8dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="@dimen/min_height_row_setting">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/ic_select_all"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:src="0x7f08087c"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="@dimen/f2"
                        android:textColor="?attr/TextColor1"
                        android:id="@+id/tv_select_all"
                        android:duplicateParentState="true"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/str_select_all"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="@dimen/f13"
                        android:textColor="?attr/TextColor2"
                        android:id="@+id/tv_select_all_desc"
                        android:duplicateParentState="true"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/str_select_all_desc"/>
                </LinearLayout>
            </LinearLayout>
            <View
                android:background="?attr/ItemSeparatorColor"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/msg_item_padding_l_r"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/ll_not_select"
                android:background="@drawable/stencils_contact_bg"
                android:paddingLeft="@dimen/msg_item_padding_l_r"
                android:paddingTop="8dp"
                android:paddingRight="@dimen/msg_item_padding_l_r"
                android:paddingBottom="8dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="@dimen/min_height_row_setting">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/ic_not_select"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:src="0x7f08087c"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="@dimen/f2"
                        android:textColor="?attr/TextColor1"
                        android:id="@+id/tv_not_select_title"
                        android:duplicateParentState="true"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/str_not_select"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="@dimen/f13"
                        android:textColor="?attr/TextColor2"
                        android:ellipsize="end"
                        android:id="@+id/tv_not_select_desc"
                        android:visibility="gone"
                        android:duplicateParentState="true"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="2"/>
                </LinearLayout>
                <ImageView
                    android:layout_gravity="center"
                    android:id="@+id/ic_show_list_choose_friend"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/hearder_func_margin_notify_number_ic_next_r"
                    android:src="0x7f080f0c"/>
                <ImageView
                    android:id="@+id/setting_ic_list"
                    android:padding="12dp"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="0x7f080c3a"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </LinearLayout>
            <View
                android:background="?attr/ItemSeparatorColor"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/msg_item_padding_l_r"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/ll_not_all"
                android:background="@drawable/stencils_contact_bg"
                android:paddingLeft="@dimen/msg_item_padding_l_r"
                android:paddingRight="@dimen/msg_item_padding_l_r"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="@dimen/min_height_row_setting">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/ic_not_all"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:src="0x7f08087c"/>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textSize="@dimen/f2"
                    android:textColor="?attr/TextColor1"
                    android:id="@+id/tv_not_all"
                    android:duplicateParentState="true"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/str_not_select_all"
                    android:layout_weight="1"/>
            </LinearLayout>
            <View
                android:background="?attr/ItemSeparatorColor"
                android:layout_width="match_parent"
                android:layout_height="1px"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
