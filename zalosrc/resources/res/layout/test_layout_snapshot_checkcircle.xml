<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/layout_test_cc_xml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:paddingEnd="64dp"
        android:layout_marginStart="16dp">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16dp">
            <com.zing.zalo.zdesign.component.CheckCircle
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Medium"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Medium"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Medium"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Medium"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:text="1"
                android:layout_marginEnd="16dp"
                app:checkedType="number"
                style="@style/CheckCircle.Medium"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:text="1"
                android:layout_marginEnd="16dp"
                app:checkedType="number"
                style="@style/CheckCircle.Medium"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16dp">
            <com.zing.zalo.zdesign.component.CheckCircle
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Small"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Small"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Small"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:layout_marginEnd="16dp"
                style="@style/CheckCircle.Small"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:text="1"
                android:layout_marginEnd="16dp"
                app:checkedType="number"
                style="@style/CheckCircle.Small"/>
            <com.zing.zalo.zdesign.component.CheckCircle
                android:enabled="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:text="1"
                android:layout_marginEnd="16dp"
                app:checkedType="number"
                style="@style/CheckCircle.Small"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:background="@color/gr60"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_marginBottom="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:elevation="@dimen/shadow_level_02">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp">
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Medium"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:enabled="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Medium"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Medium"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:enabled="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Medium"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:text="1"
                    android:layout_marginEnd="16dp"
                    app:checkedType="number"
                    style="@style/CheckCircle.Medium"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:enabled="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:text="1"
                    android:layout_marginEnd="16dp"
                    app:checkedType="number"
                    style="@style/CheckCircle.Medium"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp">
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Small"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:enabled="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Small"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Small"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:enabled="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:layout_marginEnd="16dp"
                    style="@style/CheckCircle.Small"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:text="1"
                    android:layout_marginEnd="16dp"
                    app:checkedType="number"
                    style="@style/CheckCircle.Small"/>
                <com.zing.zalo.zdesign.component.CheckCircle
                    android:enabled="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:text="1"
                    android:layout_marginEnd="16dp"
                    app:checkedType="number"
                    style="@style/CheckCircle.Small"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <FrameLayout
        android:id="@+id/layout_test_cc_dynamic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="16dp"/>
</RelativeLayout>
