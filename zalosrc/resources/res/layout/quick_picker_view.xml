<?xml version="1.0" encoding="utf-8"?>
<com.zing.zalo.p077ui.widget.layout.FixUsableHeightRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="end"
    android:orientation="vertical"
    android:id="@+id/quick_picker_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/viewFakeActionBar"
        android:layout_width="1px"
        android:layout_height="@dimen/action_bar_default_height"/>
    <include
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_default_height"
        layout="@layout/layout_action_bar"/>
    <com.zing.zalo.p077ui.widget.TouchInterceptionFrameLayout
        android:id="@+id/update_status_draggable_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/viewFakeActionBar">
        <FrameLayout
            android:id="@+id/container_location"
            android:background="?attr/ChatBarBackgroundColor"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ViewStub
            android:id="@+id/stub_chat_function_grid_panel"
            android:layout="@layout/stub_chat_compose_panel_function_grid"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ViewStub
            android:id="@+id/stub_chat_plus_panel"
            android:layout="@layout/stub_chat_plus_panel_recycler"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.zing.zalo.uicontrol.zinstant.ZinstantOAFullMenuControl
            android:id="@+id/zinstant_full_menu_control"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </com.zing.zalo.p077ui.widget.TouchInterceptionFrameLayout>
</com.zing.zalo.p077ui.widget.layout.FixUsableHeightRelativeLayout>
