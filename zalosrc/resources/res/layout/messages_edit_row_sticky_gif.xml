<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/info_contact_row"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.uicontrol.ZSimpleGIFView
        android:id="@+id/simpleGifStickyMsg"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/layoutSeeMore"
        android:background="?attr/ui_background"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/avt_image_size_default">
        <View
            android:id="@+id/extra_view_separate_top"
            android:background="?attr/ItemSeparatorColor"
            android:layout_width="match_parent"
            android:layout_height="6dp"/>
        <View
            android:id="@+id/separate_line1"
            android:background="?attr/ItemSeparatorColor"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f2"
                android:textStyle="normal"
                android:textColor="?attr/TextColor1"
                android:gravity="center_vertical"
                android:layout_gravity="center_vertical"
                android:id="@+id/tvTitleSeeMore"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/msg_item_padding_l_r"
                android:text="@string/label_see_full_search_result"
                android:singleLine="true"
                android:layout_weight="1"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f2"
                android:textStyle="normal"
                android:textColor="?attr/TextColor1"
                android:gravity="center"
                android:layout_gravity="center_vertical"
                android:id="@+id/tvTitleSeeMoreCenter"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/msg_item_padding_l_r"
                android:text="@string/label_see_full_search_result"
                android:singleLine="true"
                android:layout_weight="1"/>
            <ImageView
                android:layout_gravity="center"
                android:id="@+id/icon_next"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/msg_item_padding_l_r"
                android:layout_marginRight="@dimen/msg_item_padding_l_r"
                android:src="0x7f081044"/>
        </LinearLayout>
        <View
            android:id="@+id/separate_line2"
            android:background="?attr/ItemSeparatorColor"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/header_view_list_contact"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <View
            android:id="@+id/extra_view_item_end"
            android:background="@color/transparent"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="6dp"/>
        <View
            android:background="?attr/page_background_01"
            android:layout_width="match_parent"
            android:layout_height="10dp"/>
    </LinearLayout>
</FrameLayout>
