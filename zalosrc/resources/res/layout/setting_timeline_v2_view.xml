<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textColor="?attr/section_label"
        android:id="@+id/section_utility"
        android:background="@drawable/bg_list_item_normal"
        android:paddingLeft="16dp"
        android:paddingTop="12dp"
        android:paddingRight="16dp"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/setting_privacy_utility"
        style="@style/t_small_m"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemFilterTimeline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/setting_filter_timeline_title"/>
    <View
        android:id="@+id/section_utility_bottom_padding"
        android:background="?attr/SecondaryBackgroundColor"
        android:layout_width="match_parent"
        android:layout_height="8dp"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textColor="?attr/section_label"
        android:background="@drawable/bg_list_item_normal"
        android:paddingLeft="16dp"
        android:paddingTop="12dp"
        android:paddingRight="16dp"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/str_multi_select_option"
        style="@style/t_small_m"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemAutoPlayVideo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_setting_video_auto_play"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemAutoPlayMusic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_music_setting_auto_play_title"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemSuggestStickerComment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_suggest_sticker_feed"/>
    <View
        android:background="?attr/SecondaryBackgroundColor"
        android:layout_width="match_parent"
        android:layout_height="8dp"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textColor="?attr/section_label"
        android:id="@+id/sessionPrivacy"
        android:background="@drawable/bg_list_item_normal"
        android:paddingLeft="16dp"
        android:paddingTop="12dp"
        android:paddingRight="16dp"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/str_title_setting_private"
        style="@style/t_small_m"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemTimelineBlock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_setting_timeline_blocked_list_title"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemStoryBlock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_setting_story_blocked_list_title"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemHideFeed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_setting_timeline_hided_list_title"/>
    <View
        android:background="?attr/SecondaryBackgroundColor"
        android:layout_width="match_parent"
        android:layout_height="24dp"/>
</merge>
