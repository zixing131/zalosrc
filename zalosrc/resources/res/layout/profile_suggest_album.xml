<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fstyle="http://schemas.android.com/apk/res-auto"
    android:background="?attr/PrimaryBackgroundColor"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.uicontrol.SwipeRefreshListView
        android:id="@+id/swipe_refresh_layout"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        fstyle:modeContent="recycleview"/>
    <com.zing.zalo.feed.uicontrols.FeedRecyclerView
        android:id="@+id/feed_recycler_view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</RelativeLayout>
