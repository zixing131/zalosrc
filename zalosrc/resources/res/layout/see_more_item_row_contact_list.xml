<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="@dimen/f13"
        android:textColor="?attr/TextColor2"
        android:id="@+id/label_eol"
        android:paddingLeft="@dimen/msg_item_padding_l_r"
        android:paddingTop="8dp"
        android:paddingRight="@dimen/msg_item_padding_l_r"
        android:paddingBottom="8dp"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_tab_contact_eol"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="@dimen/f13"
        android:textColor="@color/gray_color1"
        android:id="@+id/btnSeeLess"
        android:background="@drawable/stencils_contact_bg"
        android:paddingLeft="@dimen/msg_item_padding_l_r"
        android:paddingTop="8dp"
        android:paddingRight="@dimen/msg_item_padding_l_r"
        android:paddingBottom="8dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_tab_contact_see_less"
        android:drawableRight="0x7f080d88"
        android:drawablePadding="5dp"
        android:layout_alignParentRight="true"
        app:fstyle="medium"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="@dimen/f13"
        android:textColor="@color/cLink1"
        android:id="@+id/btnSeeMore"
        android:background="@drawable/stencils_contact_bg"
        android:paddingLeft="@dimen/msg_item_padding_l_r"
        android:paddingTop="8dp"
        android:paddingRight="@dimen/msg_item_padding_l_r"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/str_zalo_contact_last_update_see_more"
        android:layout_toLeftOf="@+id/btnSeeLess"
        app:fstyle="medium"/>
</RelativeLayout>
