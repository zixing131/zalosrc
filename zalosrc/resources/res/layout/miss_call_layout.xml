<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:id="@+id/rc_root_layout"
    android:background="@color/dimColor"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/rc_main_layout"
        android:background="?attr/dialog_holo_frame"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/feed_content_padding"
        android:layout_marginRight="@dimen/feed_content_padding">
        <include layout="@layout/profile_phone_call_layout"/>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/ll_info_layout"
            android:layout_width="match_parent"
            android:layout_height="44dp">
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textColor="?attr/NotificationColor1"
                android:gravity="center"
                android:id="@+id/tv_miss_call"
                android:padding="4dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/oa_padding"
                android:text="@string/str_title_missCall"
                android:maxLines="1"
                android:drawableLeft="0x7f080a81"
                android:drawablePadding="@dimen/feed_content_padding"
                style="@style/Roboto.f2"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textColor="?attr/TextColor2"
                android:gravity="center"
                android:id="@+id/sub_line"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="4dp"
                android:text="-"
                android:maxLines="1"
                style="@style/Roboto.f2"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textColor="?attr/TextColor2"
                android:gravity="center"
                android:id="@+id/tv_time_miss_call"
                android:padding="4dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:maxLines="1"
                style="@style/Roboto.f2"/>
        </LinearLayout>
        <View
            android:background="?attr/ItemSeparatorColor"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/layout_reply"
            android:background="@drawable/stencils_contact_bg"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:weightSum="1">
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/layout_send_msg"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.5">
                <com.androidquery.util.RecyclingImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="0x7f080cf3"/>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textColor="?attr/AppPrimaryColor"
                    android:gravity="center"
                    android:duplicateParentState="true"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:text="@string/send_msg_zalouser"
                    android:singleLine="true"
                    style="@style/Roboto.f2"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/layout_callback"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.5">
                <com.androidquery.util.RecyclingImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="0x7f080cf1"/>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textColor="?attr/AppPrimaryColor"
                    android:gravity="center"
                    android:duplicateParentState="true"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:text="@string/str_msg_callback"
                    android:singleLine="true"
                    style="@style/Roboto.f2"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
