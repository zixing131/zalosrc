<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <include layout="@layout/feed_item_header_group_template"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="13dp"
        android:textColor="?attr/TextColor2"
        android:ellipsize="end"
        android:gravity="top"
        android:id="@+id/tvTagString"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/feed_content_padding"
        android:layout_marginRight="@dimen/feed_content_padding"
        android:layout_above="@+id/imvMediaThumb"
        android:layout_below="@+id/bg_feed_view"
        android:layout_alignParentLeft="true"/>
    <LinearLayout
        android:id="@+id/feed_item_social_album_timeline_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/feed_social_album_margin_lr"
        android:layout_marginTop="8dp"
        android:layout_marginRight="@dimen/feed_social_album_margin_lr"
        android:layout_marginBottom="2dp"
        android:layout_above="@+id/layoutFeedItemFooter"
        android:layout_below="@+id/tvTagString">
        <RelativeLayout
            android:layout_gravity="left"
            android:id="@+id/feed_item_social_album_timeline_group_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.zing.zalo.feed.components.FeedItemSocialAlbumHeader
                android:id="@+id/feed_item_social_album_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="48dp"/>
            <com.zing.zalo.feed.components.FeedItemSocialAlbumPhoto
                android:id="@+id/feed_item_social_album_photo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/feed_social_album_content_margin"
                android:layout_marginRight="@dimen/feed_social_album_content_margin"
                android:layout_marginBottom="@dimen/feed_social_album_content_margin"
                android:layout_below="@+id/feed_item_social_album_header"/>
            <com.zing.zalo.feed.components.FeedItemSocialAlbumVideo
                android:id="@+id/feed_item_social_album_video"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/feed_social_album_content_margin"
                android:layout_marginRight="@dimen/feed_social_album_content_margin"
                android:layout_marginBottom="@dimen/feed_social_album_content_margin"
                android:layout_below="@+id/feed_item_social_album_header"/>
        </RelativeLayout>
    </LinearLayout>
    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/feed_item_social_album_timeline_group_container"
        layout="@layout/feed_item_footer_content_social_album_group"/>
</merge>
