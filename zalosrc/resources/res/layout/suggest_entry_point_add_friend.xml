<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/ll_suggest_entry_points_add_friend"
    android:background="?attr/PrimaryBackgroundColor"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="@dimen/f0"
        android:textColor="?attr/TextColor1"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="42dp"
        android:layout_marginRight="80dp"
        android:text="@string/str_suggest_full_empty_desc"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp">
        <LinearLayout
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:id="@+id/ll_entry_point_find_friend"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <androidx.appcompat.widget.ZAppCompatImageView
                android:layout_gravity="center_horizontal"
                android:layout_width="54dp"
                android:layout_height="54dp"
                app:srcCompatEx="@drawable/chat_friend_button_addfriend"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f1s"
                android:textColor="?attr/TextColor1"
                android:gravity="center_horizontal"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/des_func_find_more_friend_zalo_tab"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:id="@+id/ll_entry_point_phonebook_suggestion"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <androidx.appcompat.widget.ZAppCompatImageView
                android:layout_gravity="center_horizontal"
                android:layout_width="54dp"
                android:layout_height="54dp"
                app:srcCompatEx="@drawable/chat_friend_button_phonebook"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f1s"
                android:textColor="?attr/TextColor1"
                android:gravity="center_horizontal"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/str_native_list_entry_view_title"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
