<?xml version="1.0" encoding="utf-8"?>
<com.zing.zalo.shortvideo.p072ui.component.p073rv.item.FollowSuggestionItem xmlns:android="http://schemas.android.com/apk/res/android" xmlns:package="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.zing.zalo.shortvideo.p072ui.widget.tv.SimpleShadowTextView
        android:textSize="16dp"
        android:textColor="@color/zch_text_primary"
        android:gravity="center_horizontal"
        android:id="@+id/txtTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="3dp"
        package:zchFontStyle="medium"/>
    <com.zing.zalo.shortvideo.p072ui.widget.tv.SimpleShadowTextView
        android:textSize="15dp"
        android:textColor="@color/zch_text_primary_a60"
        android:gravity="center_horizontal"
        android:id="@+id/txtMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="2dp"
        package:zchFontStyle="regular"/>
    <com.zing.zalo.shortvideo.p072ui.component.p073rv.item.FollowSuggestionVideoLayout
        android:id="@+id/lytVideo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.zing.zalo.shortvideo.p072ui.widget.p075rv.OverScrollableRecyclerView
        android:id="@+id/lstChannel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.zing.zalo.shortvideo.p072ui.component.p073rv.item.FollowSuggestionItem>
