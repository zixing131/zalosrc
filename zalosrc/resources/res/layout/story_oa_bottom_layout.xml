<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:id="@+id/story_views_indicator_layout"
    android:paddingTop="12dp"
    android:paddingBottom="16dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:splitMotionEvents="false">
    <androidx.appcompat.widget.ZAppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="0x7f080faf"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textColor="@color/cM0"
        android:id="@+id/tv_story_see_more"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        style="@style/t_large_m"/>
</LinearLayout>
