<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        android:id="@+id/container"
        android:paddingLeft="14dp"
        android:paddingTop="13dp"
        android:paddingRight="14dp"
        android:paddingBottom="13dp"
        android:layout_width="148dp"
        android:layout_height="156dp">
        <RelativeLayout
            android:id="@+id/layout_cover"
            android:background="@drawable/profile_album_circle"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginTop="24dp"
            android:layout_centerHorizontal="true">
            <ImageView
                android:id="@+id/thumb_empty"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/ic_see_more_profile_album"
                android:scaleType="centerInside"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textSize="14dp"
            android:textColor="?attr/AppPrimaryColor"
            android:ellipsize="end"
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="86dp"
            android:text="@string/str_profile_album_see_all_album"
            android:maxLines="1"
            android:layout_centerHorizontal="true"/>
        <FrameLayout
            android:background="@drawable/profile_ripple_effect"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </RelativeLayout>
</merge>
