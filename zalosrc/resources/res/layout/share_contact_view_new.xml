<?xml version="1.0" encoding="utf-8"?>
<com.zing.zalo.p077ui.widget.KeyboardFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:k="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/keyboard_layout"
    android:background="?attr/PrimaryBackgroundColor"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/main_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/layout_action_bar"/>
        <com.zing.zalo.uicontrol.CustomEditText
            android:textSize="16dp"
            android:textColor="?attr/TextColor1"
            android:textColorHint="?attr/TextColor2"
            android:ellipsize="end"
            android:layout_gravity="center_vertical"
            android:id="@+id/search_input_text"
            android:background="@drawable/bg_search_share_view"
            android:paddingLeft="10dp"
            android:paddingTop="6dp"
            android:paddingRight="10dp"
            android:paddingBottom="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="4dp"
            android:hint="@string/str_hint_search_tab_zalo_share_contact"
            android:maxLines="1"
            android:singleLine="true"
            android:drawableLeft="@drawable/ic_searchbox_n"
            android:drawablePadding="10dp"
            android:inputType="textMultiLine"
            android:textCursorDrawable="@drawable/chat_bar_text_cursor"/>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">
            <ListView
                android:id="@+id/phoneList"
                android:scrollbars="vertical"
                android:fadingEdge="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:listSelector="@color/transparent"
                android:cacheColorHint="#00000000"
                android:divider="#00000000"
                android:dividerHeight="0dp"
                android:choiceMode="singleChoice"/>
            <com.zing.zalo.p077ui.zviews.multistate.MultiStateView
                android:id="@+id/multi_state"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                k:msvEmptyLayout="@layout/layout_empty_state"
                k:msvErrorLayout="@layout/layout_error_state"/>
        </FrameLayout>
        <View
            android:id="@+id/fake_space_footer"
            android:visibility="visible"
            android:layout_width="match_parent"
            android:layout_height="0dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_gravity="bottom"
        android:orientation="vertical"
        android:id="@+id/footer_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/bubble_footer"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:src="0x7f081174"
                android:scaleType="fitXY"/>
            <LinearLayout
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/content_bubble_footer"
                android:background="?attr/ChatBarBackgroundColor"
                android:paddingLeft="16dp"
                android:paddingTop="2dp"
                android:paddingRight="6dp"
                android:paddingBottom="2dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.recyclerview.widget.RecyclerView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/rv_bubbles"
                    android:visibility="visible"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"/>
                <androidx.appcompat.widget.ZAppCompatImageView
                    android:layout_gravity="center"
                    android:id="@+id/btn_done_add_item"
                    android:layout_width="62dp"
                    android:layout_height="62dp"
                    k:srcCompatEx="?attr/floating_action_btn_share"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/layout_bottom"
            android:visibility="visible"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/layout_bottom_shadow"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:src="0x7f081174"
                android:scaleType="fitXY"/>
            <View
                android:id="@+id/layout_bottom_line"
                android:background="?attr/HeaderFormBottomLineColor"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/layout_bottom_content"
                android:background="?attr/ChatBarBackgroundColor"
                android:padding="8dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textSize="@dimen/f0"
                    android:textColor="?attr/ChatTextColor1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/str_include_phone_number_share_contact"/>
                <com.zing.zalo.p077ui.widget.StencilSwitch
                    android:layout_gravity="center"
                    android:id="@+id/sw_show_phone_number"
                    android:duplicateParentState="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/hearder_func_margin_notify_number_ic_next_r"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</com.zing.zalo.p077ui.widget.KeyboardFrameLayout>
