<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/camera_overflow_layout"
    android:paddingRight="8dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_gravity="top|right"
        android:orientation="vertical"
        android:background="@drawable/background_camera_editor_option"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/camera_more_video_speed_layout"
            android:background="?attr/selectableItemBackground"
            android:paddingLeft="12dp"
            android:paddingTop="8dp"
            android:paddingRight="12dp"
            android:paddingBottom="8dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="128dp"
            android:minHeight="40dp">
            <com.androidquery.util.RecyclingImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                app:srcCompatEx="@drawable/ic_video_speed_camera_more"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f0"
                android:textColor="?attr/TextColor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="@string/str_editor_overflow_video_speed"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/camera_more_photo_crop_layout"
            android:background="?attr/selectableItemBackground"
            android:paddingLeft="12dp"
            android:paddingTop="8dp"
            android:paddingRight="12dp"
            android:paddingBottom="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="128dp"
            android:minHeight="40dp">
            <com.androidquery.util.RecyclingImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                app:srcCompatEx="@drawable/ic_photo_crop_camera_more"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f0"
                android:textColor="?attr/TextColor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="@string/str_editor_overflow_photo_crop"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/camera_more_doodle_layout"
            android:background="?attr/selectableItemBackground"
            android:paddingLeft="12dp"
            android:paddingTop="8dp"
            android:paddingRight="12dp"
            android:paddingBottom="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="128dp"
            android:minHeight="40dp">
            <com.androidquery.util.RecyclingImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                app:srcCompatEx="@drawable/ic_doodle_camera_more"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f0"
                android:textColor="?attr/TextColor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="@string/str_editor_overflow_doodle"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/camera_more_filter_layout"
            android:background="?attr/selectableItemBackground"
            android:paddingLeft="12dp"
            android:paddingTop="8dp"
            android:paddingRight="12dp"
            android:paddingBottom="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="128dp"
            android:minHeight="40dp">
            <com.androidquery.util.RecyclingImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                app:srcCompatEx="@drawable/ic_filter_camera_more"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f0"
                android:textColor="?attr/TextColor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="@string/str_editor_overflow_filter"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/camera_more_location_layout"
            android:background="?attr/selectableItemBackground"
            android:paddingLeft="12dp"
            android:paddingTop="8dp"
            android:paddingRight="12dp"
            android:paddingBottom="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="128dp"
            android:minHeight="40dp">
            <com.androidquery.util.RecyclingImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                app:srcCompatEx="@drawable/ic_location_camera_more"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f0"
                android:textColor="?attr/TextColor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="@string/str_editor_overflow_location"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/camera_more_save_layout"
            android:background="?attr/selectableItemBackground"
            android:paddingLeft="12dp"
            android:paddingTop="8dp"
            android:paddingRight="12dp"
            android:paddingBottom="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="128dp"
            android:minHeight="40dp">
            <com.androidquery.util.RecyclingImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                app:srcCompatEx="@drawable/ic_save_camera_more"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f0"
                android:textColor="?attr/TextColor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="@string/str_editor_overflow_save"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
