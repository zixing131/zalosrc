<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <com.androidquery.util.RecyclingImageView
        android:id="@+id/avatar_on_action_bar"
        android:layout_width="@dimen/storage_avatar_action_bar_w"
        android:layout_height="@dimen/storage_avatar_action_bar_h"
        android:src="0x7f080958"
        android:layout_centerVertical="true"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_gravity="bottom|right"
        android:id="@+id/status_icon"
        android:layout_width="12dp"
        android:layout_height="12dp"
        app:srcCompat="@drawable/ic_oa_verify"/>
</FrameLayout>
