<?xml version="1.0" encoding="utf-8"?>
<com.zing.zalo.p077ui.widget.ImeFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:k="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/keyboard_layout"
    android:background="?attr/PrimaryBackgroundColor"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/main_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/main_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.zing.zalo.zdesign.component.header.ZdsActionBar
                android:id="@+id/zds_action_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                k:leadingType="back"
                k:middleTitleText="@string/str_search_global_pre_state_quick_access_header_label"
                k:middleType="title"
                k:trailingType="none"
                style="@style/BlueZdsActionBar.AppType"/>
            <com.zing.zalo.uicontrol.CustomEditText
                android:textSize="16dp"
                android:textColor="?attr/TextColor1"
                android:textColorHint="?attr/TextColor2"
                android:ellipsize="end"
                android:layout_gravity="center_vertical"
                android:id="@+id/search_input_text"
                android:background="@drawable/bg_search_share_view"
                android:paddingLeft="10dp"
                android:paddingTop="6dp"
                android:paddingRight="10dp"
                android:paddingBottom="6dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="8dp"
                android:hint="@string/str_search_global_pre_state_quick_access_search_hint"
                android:maxLines="1"
                android:singleLine="true"
                android:drawableLeft="@drawable/ic_searchbox_n"
                android:drawablePadding="10dp"
                android:inputType="textMultiLine"
                android:textCursorDrawable="@drawable/chat_bar_text_cursor"
                k:drawableTint="?attr/icon_01"/>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
                <com.zing.zalo.p077ui.searchglobal.widget.SearchGlobalPreStateLayout
                    android:id="@+id/listLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.zing.zalo.p077ui.zviews.multistate.MultiStateView
                    android:id="@+id/multi_state"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    k:msvEmptyLayout="@layout/layout_empty_state"
                    k:msvErrorLayout="@layout/layout_error_state"/>
            </FrameLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_gravity="bottom"
            android:orientation="vertical"
            android:id="@+id/footer_layout"
            android:background="@drawable/res_call_bg_footer_create_group_call"
            android:focusable="true"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:elevation="24dp">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="16dp"
                android:paddingTop="12dp"
                android:paddingRight="16dp"
                android:paddingBottom="4dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textSize="13dp"
                    android:textColor="?attr/TextColor2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/str_call_groupcall_ready_list_title"
                    android:lines="1"
                    android:singleLine="true"
                    android:layout_marginEnd="3dp"/>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textSize="14dp"
                    android:textColor="?attr/TextColor1"
                    android:id="@+id/selectCountTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    k:fstyle="medium"/>
            </LinearLayout>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="13dp"
                android:textColor="?attr/TextColor1"
                android:id="@+id/toastTextView"
                android:paddingLeft="16dp"
                android:paddingTop="4dp"
                android:paddingRight="16dp"
                android:paddingBottom="4dp"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:lines="1"
                k:fstyle="medium"/>
            <com.zing.zalo.p077ui.searchglobal.widget.SearchGlobalPreStateLayout
                android:layout_gravity="center_vertical"
                android:id="@+id/bubbleListLayout"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="64dp"/>
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="14dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/btn_done_add_item"
                android:paddingTop="11dp"
                android:paddingBottom="11dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="16dp"
                android:text="@string/str_positive_button_join_question_dialog"
                android:lines="1"
                android:singleLine="true"
                style="@style/btnType1.medium"/>
        </LinearLayout>
    </FrameLayout>
</com.zing.zalo.p077ui.widget.ImeFrameLayout>
