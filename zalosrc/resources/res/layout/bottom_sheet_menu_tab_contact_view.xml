<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemAlias"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_popup_option_change_alias"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemFavorite"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_popup_option_close_friend"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemBlock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_block_manager"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemDelete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_popup_option_delete_friend"/>
</LinearLayout>
