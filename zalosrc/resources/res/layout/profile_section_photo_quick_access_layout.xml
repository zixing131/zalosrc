<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/quick_access_layout_container"
    android:background="?attr/ProfilePrimaryBackgroundColor"
    android:paddingTop="12dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.feed.uicontrols.FeedRecyclerView
        android:id="@+id/section_photo_quick_access_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.zing.zalo.feed.mvp.profile.ProfileSkeletonView
        android:id="@+id/quick_access_skeleton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</RelativeLayout>
