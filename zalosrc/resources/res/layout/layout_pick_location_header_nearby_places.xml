<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="?attr/PopupBackgroundColor"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:background="?attr/TabBottomLineColor"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="13dp"
        android:textColor="?attr/ChatTextColor1"
        android:id="@+id/tv_nearby_locations"
        android:paddingLeft="16dp"
        android:paddingTop="10dp"
        android:paddingBottom="6dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_share_location_nearby_places_2"
        app:fstyle="medium"/>
</LinearLayout>
