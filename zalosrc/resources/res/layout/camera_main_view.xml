<?xml version="1.0" encoding="utf-8"?>
<com.zing.zalo.uicomponents.framelayout.DragToCloseLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/camera_drag_to_close_layout"
    android:focusable="true"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:splitMotionEvents="false">
    <FrameLayout
        android:id="@+id/status_view"
        android:background="@color/black"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.zing.zalo.p077ui.widget.DragCameraLayout
        android:id="@+id/camera_drag_layout"
        android:focusable="true"
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout
            android:id="@+id/camera_resizable_layout"
            android:background="@color/black"
            android:focusable="true"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </com.zing.zalo.p077ui.widget.DragCameraLayout>
</com.zing.zalo.uicomponents.framelayout.DragToCloseLayout>
