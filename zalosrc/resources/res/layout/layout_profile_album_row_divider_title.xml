<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:background="?attr/PrimaryBackgroundColor"
    android:paddingLeft="@dimen/feed_padding_left"
    android:paddingRight="@dimen/feed_padding_right"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textSize="16dp"
        android:textColor="?attr/TextColor1"
        android:ellipsize="end"
        android:gravity="left"
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/feed_padding_top"
        android:layout_marginBottom="@dimen/feed_padding_bottom"
        android:maxLines="1"
        app:fstyle="medium"/>
</RelativeLayout>
