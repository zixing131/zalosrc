<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:background="@drawable/dialog_corner_bg_10"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginRight="20dp">
    <RelativeLayout
        android:paddingLeft="22dp"
        android:paddingTop="15dp"
        android:paddingRight="22dp"
        android:paddingBottom="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textColor="@color/cMTitle1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="UI Style"
            android:includeFontPadding="false"
            android:layout_centerVertical="true"
            app:fstyle="light"
            style="@style/Roboto.f6"/>
        <Spinner
            android:textColor="@color/cMTitle1"
            android:entries="@array/call_ui_style"
            android:id="@+id/ui_style_spinner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawSelectorOnTop="true"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View
        android:background="@color/dialog_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"/>
    <View
        android:background="@color/dialog_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"/>
    <RelativeLayout
        android:paddingLeft="22dp"
        android:paddingTop="15dp"
        android:paddingRight="22dp"
        android:paddingBottom="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textColor="@color/cMTitle1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Fly Emoji"
            android:includeFontPadding="false"
            android:layout_centerVertical="true"
            app:fstyle="light"
            style="@style/Roboto.f6"/>
        <Switch
            android:id="@+id/enable_liveemoji_videocall_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View
        android:background="@color/dialog_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"/>
    <View
        android:background="@color/dialog_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"/>
    <View
        android:background="@color/dialog_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"/>
    <RelativeLayout
        android:paddingLeft="22dp"
        android:paddingTop="15dp"
        android:paddingRight="22dp"
        android:paddingBottom="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textColor="@color/cMTitle1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Accept Btn"
            android:includeFontPadding="false"
            android:layout_centerVertical="true"
            app:fstyle="light"
            style="@style/Roboto.f6"/>
        <Switch
            android:id="@+id/enable_new_accept_end_btn_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View
        android:background="@color/dialog_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"/>
    <RelativeLayout
        android:paddingLeft="22dp"
        android:paddingTop="15dp"
        android:paddingRight="22dp"
        android:paddingBottom="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.zing.zalo.p077ui.widget.RobotoTextView
            android:textColor="@color/cMTitle1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enable Text Support"
            android:includeFontPadding="false"
            android:layout_centerVertical="true"
            app:fstyle="light"
            style="@style/Roboto.f6"/>
        <Switch
            android:id="@+id/enable_text_support"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View
        android:background="@color/dialog_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:paddingLeft="22dp"
        android:paddingTop="15dp"
        android:paddingRight="22dp"
        android:paddingBottom="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.zing.zalo.p077ui.widget.RobotoButton
            android:id="@+id/btn_cancel"
            android:layout_width="0dp"
            android:minWidth="205dp"
            android:text="Cancel"
            android:layout_weight="1"
            style="@style/btnType2.medium.camera"/>
        <com.zing.zalo.p077ui.widget.RobotoButton
            android:textColor="@drawable/stencils_button_style3_text"
            android:id="@+id/btn_ok"
            android:layout_width="0dp"
            android:minWidth="205dp"
            android:text="OK"
            android:layout_weight="1"
            style="@style/btnType1.medium"/>
    </LinearLayout>
</LinearLayout>
