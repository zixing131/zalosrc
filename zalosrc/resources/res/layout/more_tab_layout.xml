<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tf="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/root_backgroundmain"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.zing.zalo.zinstant.component.p081ui.scrollview.ZinstantScrollViewImpl
        android:id="@+id/parentScrollView"
        android:background="?attr/PrimaryBackgroundColor"
        android:scrollbars="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">
        <LinearLayout
            android:gravity="top"
            android:orientation="vertical"
            android:id="@+id/layout_child_scrollView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/rlayout_myInfo"
                android:background="@drawable/bg_with_bottom_border_selector"
                android:paddingLeft="@dimen/msg_item_padding_l_r"
                android:paddingTop="@dimen/hearder_func_margin_content_t_b"
                android:paddingBottom="@dimen/hearder_func_margin_content_t_b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="72dp">
                <com.zing.zalo.p077ui.widget.AvatarImageView
                    android:id="@+id/image_profile"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginRight="@dimen/margin_section_item"
                    android:src="0x7f080958"
                    android:layout_centerVertical="true"
                    style="@style/AvatarImageViewWithBorder"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="vertical"
                    android:id="@+id/name_status_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="12dp"
                    android:layout_toLeftOf="@+id/btn_my_qr_code"
                    android:layout_toRightOf="@+id/image_profile"
                    android:layout_centerVertical="true">
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="@dimen/f2"
                        android:textColor="@color/cmtxt1_itemtext_colorstate"
                        android:layout_gravity="center"
                        android:id="@+id/tv_dpn"
                        android:paddingLeft="7dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:singleLine="true"/>
                    <LinearLayout
                        android:layout_gravity="center_vertical"
                        android:id="@+id/tv_status_container"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <com.zing.zalo.feed.components.GlowingReddot
                            android:id="@+id/glowing_reddot"
                            android:visibility="gone"
                            android:layout_width="20dp"
                            android:layout_height="20dp"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="@dimen/f7"
                            android:textColor="?attr/TextColor2"
                            android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            android:id="@+id/tv_mystatus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:singleLine="true"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView
                    android:id="@+id/setting_ic_next"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="0x7f080f45"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
                <ImageButton
                    android:id="@+id/btn_my_qr_code"
                    android:background="@drawable/stencils_contact_bg"
                    android:visibility="gone"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="0x7f080f3a"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
                <FrameLayout
                    android:id="@+id/ll_switch_account"
                    android:paddingLeft="12dp"
                    android:paddingTop="8dp"
                    android:paddingRight="16dp"
                    android:paddingBottom="8dp"
                    android:visibility="visible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true">
                    <com.zing.zalo.p077ui.widget.GroupAvatarView
                        android:id="@+id/switch_account"
                        android:layout_width="28dp"
                        android:layout_height="28dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="2dp"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="@dimen/f5"
                        android:textColor="?attr/PrimaryBackgroundColor"
                        android:gravity="center"
                        android:id="@+id/tv_switch_account"
                        android:background="0x7f080958"
                        android:layout_width="28dp"
                        android:layout_height="28dp"
                        android:layout_marginLeft="26dp"
                        android:layout_marginTop="2dp"
                        android:text="+4"/>
                    <com.zing.zalo.uicontrol.CircleImage
                        android:id="@+id/bg_switch_account_2"
                        android:layout_width="32dp"
                        android:layout_height="32dp"/>
                    <com.zing.zalo.p077ui.widget.GroupAvatarView
                        android:id="@+id/switch_account_2"
                        android:layout_width="28dp"
                        android:layout_height="28dp"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                </FrameLayout>
            </RelativeLayout>
            <com.zing.zalo.feed.mvp.profile.DiscoverySkeletonView
                android:id="@+id/skeletonView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/layout_monitor_perf_container"
                android:paddingTop="@dimen/msg_item_padding_t_b"
                android:paddingBottom="@dimen/msg_item_padding_t_b"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="@dimen/feed_avatar_size"
                    android:layout_height="@dimen/feed_avatar_size"
                    android:layout_marginLeft="@dimen/msg_item_padding_l_r"
                    android:layout_marginTop="@dimen/msg_item_padding_t_b"
                    android:layout_marginRight="@dimen/msg_item_padding_l_r"
                    android:layout_marginBottom="@dimen/msg_item_padding_t_b"
                    android:src="0x7f0804d8"
                    android:scaleType="fitXY"
                    android:contentDescription="@string/nothing"
                    style="@style/myImageView"/>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textSize="@dimen/f2"
                    android:textColor="@color/cmtxt1_itemtext_colorstate"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/stencils_contact_bg"
                    android:visibility="visible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="13dp"
                    android:text="Performance"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/elements_feature"
                android:background="?attr/SecondaryBackgroundColor"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="vertical"
                    android:background="@drawable/bg_with_top_bottom_border"
                    android:paddingTop="@dimen/text_header_list_margin_t_b"
                    android:paddingBottom="@dimen/text_header_list_margin_t_b"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/separate_line_big_height">
                    <View
                        android:id="@+id/separate_line_nearby"
                        android:background="?attr/ItemSeparatorColor"
                        android:layout_width="wrap_content"
                        android:layout_height="1px"
                        android:layout_marginLeft="72dp"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:id="@+id/layout_nearby_container"
                        android:background="@drawable/stencils_contact_bg"
                        android:paddingTop="@dimen/msg_item_padding_t_b"
                        android:paddingBottom="@dimen/msg_item_padding_t_b"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_gravity="center_vertical"
                            android:layout_width="@dimen/feed_avatar_size"
                            android:layout_height="@dimen/feed_avatar_size"
                            android:layout_marginLeft="@dimen/msg_item_padding_l_r"
                            android:layout_marginRight="@dimen/msg_item_padding_l_r"
                            android:src="0x7f0804da"
                            android:scaleType="fitXY"
                            android:contentDescription="@string/nothing"
                            style="@style/myImageView"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="@dimen/f2"
                            android:textColor="@color/cmtxt1_itemtext_colorstate"
                            android:gravity="center_vertical"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/stencils_contact_bg"
                            android:visibility="visible"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="13dp"
                            android:text="@string/str_nearby_title"
                            android:layout_weight="1"/>
                    </LinearLayout>
                    <View
                        android:id="@+id/separate_line_qr_code"
                        android:background="?attr/ItemSeparatorColor"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="1px"
                        android:layout_marginLeft="72dp"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:id="@+id/layout_qrcode_container"
                        android:background="@drawable/stencils_contact_bg"
                        android:paddingTop="@dimen/msg_item_padding_t_b"
                        android:paddingBottom="@dimen/msg_item_padding_t_b"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <androidx.appcompat.widget.ZAppCompatImageView
                            android:layout_gravity="center_vertical"
                            android:layout_width="@dimen/feed_avatar_size"
                            android:layout_height="@dimen/feed_avatar_size"
                            android:layout_marginLeft="@dimen/msg_item_padding_l_r"
                            android:layout_marginRight="@dimen/msg_item_padding_l_r"
                            android:scaleType="fitXY"
                            android:contentDescription="@string/nothing"
                            tf:srcCompatEx="@drawable/ava_ketnoi_maqr"
                            style="@style/myImageView"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="@dimen/f2"
                            android:textColor="@color/cmtxt1_itemtext_colorstate"
                            android:gravity="center_vertical"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/stencils_contact_bg"
                            android:visibility="visible"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="13dp"
                            android:text="@string/qrcode_scan_qrcode"
                            android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/feature_html_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/layoutAdsBanner"
                    android:background="@drawable/stencils_contact_bg"
                    android:paddingLeft="@dimen/msg_item_padding_l_r"
                    android:paddingTop="12dp"
                    android:paddingRight="@dimen/msg_item_padding_l_r"
                    android:paddingBottom="12dp"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.androidquery.util.RecyclingImageView
                        android:id="@+id/imvAdsAvatar"
                        android:layout_width="@dimen/feed_avatar_size"
                        android:layout_height="@dimen/feed_avatar_size"
                        android:scaleType="centerCrop"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/msg_item_padding_l_r"
                        android:layout_marginRight="@dimen/msg_item_padding_l_r"
                        android:layout_weight="1">
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="@dimen/f2"
                            android:textColor="?attr/TextColor1"
                            android:id="@+id/tvAdsTitle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            tf:fstyle="medium"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="@dimen/f71"
                            android:textColor="?attr/TextColor1"
                            android:id="@+id/tvAdsDesc"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLines="2"/>
                    </LinearLayout>
                    <com.androidquery.util.RecyclingImageView
                        android:id="@+id/imvHideAdMenu"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="0x7f081044"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:background="?attr/PrimaryBackgroundColor"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/separate_line_big_height">
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="@dimen/f2"
                        android:textColor="@color/cmtxt1_itemtext_colorstate"
                        android:gravity="center_vertical"
                        android:layout_gravity="center_vertical"
                        android:id="@+id/layoutratezalo"
                        android:background="@drawable/stencils_contact_bg"
                        android:paddingLeft="@dimen/msg_item_padding_l_r"
                        android:paddingRight="@dimen/msg_item_padding_l_r"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/func_bar_general_h"
                        android:text="@string/str_ratezalo"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </com.zing.zalo.zinstant.component.p081ui.scrollview.ZinstantScrollViewImpl>
    <FrameLayout
        android:layout_gravity="bottom"
        android:id="@+id/layoutFooterFunction"
        android:background="@drawable/stencils_bg_footer_function_maintab"
        android:visibility="gone"
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="@dimen/abs__action_bar_default_height">
        <ImageButton
            android:layout_gravity="left"
            android:id="@+id/imgButtonPrivacy"
            android:background="@drawable/stencils_bg_btn_sidemenu"
            android:paddingLeft="@dimen/footer_fucn_margin_size"
            android:paddingRight="@dimen/footer_fucn_margin_size"
            android:focusable="false"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/abs__action_bar_default_height"
            android:src="0x7f0811e3"
            android:contentDescription="@string/str_desImg"/>
        <ImageButton
            android:layout_gravity="right"
            android:id="@+id/imgButtonSetting"
            android:background="@drawable/stencils_bg_btn_sidemenu"
            android:paddingLeft="@dimen/footer_fucn_margin_size"
            android:paddingRight="@dimen/footer_fucn_margin_size"
            android:focusable="false"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/abs__action_bar_default_height"
            android:src="0x7f0811e4"
            android:contentDescription="@string/str_desImg"/>
    </FrameLayout>
</FrameLayout>
