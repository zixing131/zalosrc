<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:id="@+id/parenLayout_alert_dlg"
    android:layout_width="328dp"
    android:layout_height="179dp"
    android:minWidth="0x7f0701d8">
    <View
        android:layout_gravity="bottom"
        android:background="@drawable/white_background_with_rounded"
        android:layout_width="match_parent"
        android:layout_height="150dp"/>
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textColor="@color/cMtxt2"
        android:gravity="center"
        android:layout_gravity="bottom"
        android:id="@+id/message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="59dp"
        android:layout_marginRight="58dp"
        android:layout_marginBottom="66dp"
        android:text="@string/showcase_camera_location_permission_request_message"
        style="@style/Roboto.f0"/>
    <com.zing.zalo.p077ui.widget.RobotoButton
        android:textColor="@color/cMtxt2"
        android:gravity="center"
        android:layout_gravity="bottom|left"
        android:id="@+id/button2"
        android:background="@drawable/material_button_dialog_bg"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginLeft="70dp"
        android:layout_marginBottom="10dp"
        android:minWidth="64dp"
        android:text="@string/str_cancel"
        android:textAllCaps="true"
        style="@style/Roboto.f0"/>
    <com.zing.zalo.p077ui.widget.RobotoButton
        android:textColor="@color/cM4"
        android:gravity="center"
        android:layout_gravity="bottom|right"
        android:id="@+id/button1"
        android:background="@drawable/material_button_dialog_bg"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginRight="70dp"
        android:layout_marginBottom="10dp"
        android:minWidth="64dp"
        android:text="@string/str_ok"
        android:textAllCaps="true"
        style="@style/Roboto.f0"/>
    <ImageView
        android:layout_gravity="top|center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="0x7f080bf5"
        android:scaleType="fitCenter"/>
</FrameLayout>
