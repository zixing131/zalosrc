<?xml version="1.0" encoding="utf-8"?>
<com.zing.zalo.p077ui.widget.CustomRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/update_status_main_layout"
    android:background="@drawable/white"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_action_bar"/>
    <com.androidquery.util.RecyclingImageView
        android:id="@+id/decor_feed_top"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/abc_action_bar_default_height_material"
        android:scaleType="centerCrop"
        android:layout_above="@+id/changable_height_layout"/>
    <com.zing.zalo.p077ui.widget.ChangeableHeightRelativeLayout
        android:id="@+id/changable_height_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/zalo_action_bar">
        <com.zing.zalo.p077ui.widget.AspectRatioImageView
            android:id="@+id/decor_feed_bottom"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/layout_typo_location_tag_control"/>
        <com.zing.zalo.social.widget.CustomScrollView
            android:id="@+id/status_scroll_view"
            android:fadingEdgeLength="0dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:layout_above="@+id/updateStatusFooterDivider">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/ll_preview_post_feed"
                android:paddingBottom="@dimen/layout_postfeed_tag_location_height"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <com.zing.zalo.feed.components.FeedBackgroundView
                    android:id="@+id/bg_feed_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.zing.zalo.feed.mvp.postfeed.component.ControlComposeButtonView
                    android:id="@+id/compose_button_header"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/layout_album"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp">
                    <RelativeLayout
                        android:id="@+id/container_detail_album"
                        android:background="@drawable/bg_btn_selected_album"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.androidquery.util.RecyclingImageView
                            android:id="@+id/theme_decor"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:scaleType="fitEnd"
                            android:layout_toLeftOf="@+id/container_detail_album_arrow_right"
                            android:layout_alignTop="@+id/title"
                            android:layout_alignBottom="@+id/desc"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="10dp"
                            android:textColor="?attr/TextColor2"
                            android:id="@+id/title"
                            android:paddingTop="16dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/album"
                            android:textAllCaps="true"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="14dp"
                            android:textColor="?attr/TextColor1"
                            android:ellipsize="end"
                            android:id="@+id/desc"
                            android:paddingBottom="16dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="42dp"
                            android:maxLines="1"
                            android:layout_below="@+id/title"
                            android:layout_alignLeft="@+id/title"/>
                        <ImageView
                            android:id="@+id/container_detail_album_arrow_right"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_chevron_down_line_16"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"/>
                    </RelativeLayout>
                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:id="@+id/empty_photo_album"
                        android:background="@drawable/bg_btn_select_album_photo"
                        android:padding="38dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginBottom="16dp"
                            android:src="@drawable/icn_plus_photo_to_album"
                            android:scaleType="centerCrop"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="14dp"
                            android:textColor="?attr/TextColor2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/str_profile_album_add_photo_to_album"/>
                    </LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    android:id="@+id/preview_layout"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.zing.zalo.uidrawing.ModulesView
                        android:id="@+id/background_feed_music_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/preview_content"
                        android:layout_alignTop="@+id/preview_content"
                        android:layout_alignRight="@+id/preview_content"
                        android:layout_alignBottom="@+id/preview_content"/>
                    <com.zing.zalo.uidrawing.ModulesView
                        android:id="@+id/overlay_background_feed_photo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/background_feed_music_container"
                        android:layout_alignTop="@+id/background_feed_music_container"
                        android:layout_alignRight="@+id/background_feed_music_container"
                        android:layout_alignBottom="@+id/background_feed_music_container"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/preview_content"
                        android:paddingTop="12dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.zing.zalo.feed.uicontrols.FeedGridModulesView
                            android:layout_gravity="center_horizontal"
                            android:id="@+id/photo_preview_grid"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:orientation="horizontal"
                            android:id="@+id/empty_photo_music"
                            android:background="@drawable/bg_compose_music_empty_photo"
                            android:padding="38dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="24dp"
                            android:layout_marginRight="24dp">
                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_marginRight="8dp"
                                android:src="@drawable/icn_postfeed_add_photo_music"
                                android:scaleType="centerCrop"/>
                            <com.zing.zalo.p077ui.widget.RobotoTextView
                                android:textSize="14dp"
                                android:textColor="@color/white"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/str_compose_feed_add_photo"/>
                        </LinearLayout>
                        <com.zing.zalo.feed.mvp.postfeed.component.BoxLyric
                            android:id="@+id/box_lyric"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <FrameLayout
                        android:id="@+id/song_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/preview_content">
                        <com.zing.zalo.feed.components.ComposeSongItemView
                            android:id="@+id/song_view"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="24dp"
                            android:layout_marginRight="24dp"
                            android:layout_marginBottom="16dp"/>
                    </FrameLayout>
                </RelativeLayout>
                <com.zing.zalo.zdesign.component.Button
                    android:id="@+id/btn_photo_grid_add"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/feed_padding_left"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="@dimen/feed_padding_right"
                    android:layout_marginBottom="8dp"
                    android:text="@string/str_fpf_add_photo"
                    app:btnSupportiveIcon="@drawable/zds_ic_plus_line_16"
                    app:btnSupportiveIconPosition="leading"
                    style="@style/ButtonSmall.SecondaryNeutral"/>
                <View
                    android:id="@+id/statusAnchor"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"/>
                <HorizontalScrollView
                    android:id="@+id/status_photos_grid"
                    android:visibility="gone"
                    android:scrollbars="none"
                    android:layout_width="match_parent"
                    android:layout_height="78dp"
                    android:layout_marginLeft="@dimen/feed_padding_left"
                    android:divider="@color/chat_gallery_background_color"/>
                <RelativeLayout
                    android:layout_gravity="center"
                    android:id="@+id/attachment_sticker_preview_tab"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginBottom="8dp">
                    <com.zing.zalo.stickers.FeedStickerView
                        android:id="@+id/feed_sticker"
                        android:layout_width="@dimen/sticker_size_update_status"
                        android:layout_height="@dimen/sticker_size_update_status"
                        android:layout_centerInParent="true"/>
                    <View
                        android:id="@+id/feed_sticker_preview_delete"
                        android:background="0x7f080c36"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginLeft="6dp"
                        android:layout_toRightOf="@+id/feed_sticker"
                        android:layout_alignTop="@+id/feed_sticker"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_gravity="left"
                    android:id="@+id/layoutLinkTab"
                    android:background="?attr/PopupBackgroundColor"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/link_header_height"
                    android:layout_marginLeft="@dimen/feed_padding_left"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="@dimen/oa_padding"
                    android:layout_marginBottom="8dp">
                    <com.androidquery.util.RecyclingImageView
                        android:id="@+id/imvLinkHeader"
                        android:layout_width="64dp"
                        android:layout_height="64dp"
                        android:src="@drawable/attach_gallery_96"
                        android:scaleType="centerCrop"
                        android:adjustViewBounds="true"/>
                    <ProgressBar
                        android:theme="@style/CircularProgress"
                        android:id="@+id/pb_link_progress"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="24dp"
                        android:indeterminate="true"
                        android:indeterminateDuration="1500"
                        android:indeterminateBehavior="repeat"
                        android:layout_centerInParent="true"
                        style="@style/ProgressBarCircleNormal"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textColor="?attr/TextColor1"
                        android:ellipsize="end"
                        android:id="@+id/tvLinkTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="9dp"
                        android:layout_marginRight="18dp"
                        android:maxLines="1"
                        android:layout_toLeftOf="@+id/link_preview_delete"
                        android:layout_toRightOf="@+id/imvLinkHeader"
                        style="@style/Roboto.f5b"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textColor="?attr/TextColor2"
                        android:ellipsize="end"
                        android:id="@+id/tvLinkDescription"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="18dp"
                        android:maxLines="1"
                        android:layout_toRightOf="@+id/imvLinkHeader"
                        android:layout_below="@+id/tvLinkTitle"
                        style="@style/Roboto.f5"/>
                    <View
                        android:layout_gravity="top|right"
                        android:id="@+id/link_preview_delete"
                        android:background="@drawable/icn_close_preview_link"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_margin="6dp"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"/>
                </RelativeLayout>
                <FrameLayout
                    android:id="@+id/layoutVideoFeed"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/feed_padding_left"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="@dimen/feed_padding_right"
                    android:layout_marginBottom="8dp">
                    <com.zing.zalo.p077ui.widget.AspectRatioImageView
                        android:id="@+id/imv_thumb_video"
                        android:layout_width="match_parent"
                        android:layout_height="176dp"
                        android:src="@drawable/attach_gallery_96"
                        android:scaleType="centerCrop"
                        android:adjustViewBounds="true"/>
                    <ImageView
                        android:layout_gravity="center"
                        android:duplicateParentState="true"
                        android:layout_width="54dp"
                        android:layout_height="54dp"
                        android:src="@drawable/icn_csc_play_big_selector"
                        android:scaleType="fitCenter"/>
                    <ImageView
                        android:layout_gravity="right"
                        android:id="@+id/imv_video_feed_remove"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:src="0x7f080c36"
                        android:scaleType="center"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="10dp"
                        android:textColor="@color/white"
                        android:gravity="center_vertical"
                        android:layout_gravity="bottom|left"
                        android:id="@+id/tv_video_duration"
                        android:background="@drawable/bg_video_duration"
                        android:paddingLeft="7dp"
                        android:paddingRight="7dp"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:layout_marginBottom="12dp"/>
                </FrameLayout>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:gravity="center_vertical"
                    android:id="@+id/tv_tag_location_review"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/feed_padding_left"
                    android:layout_marginRight="@dimen/feed_padding_right"
                    android:layout_marginBottom="8dp"
                    style="@style/Roboto.f2b"/>
                <RelativeLayout
                    android:layout_gravity="left"
                    android:id="@+id/layoutThumbLocation"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/feed_padding_left"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="@dimen/feed_padding_right"
                    android:layout_marginBottom="8dp">
                    <com.zing.zalo.p077ui.widget.AspectRatioImageView
                        android:id="@+id/imv_thumb_location"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/bg_feed"
                        android:scaleType="centerCrop"
                        android:adjustViewBounds="true"/>
                    <ImageView
                        android:layout_gravity="right"
                        android:id="@+id/ic_thumb_location_remove"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:src="0x7f080c36"
                        android:scaleType="center"
                        android:layout_alignTop="@+id/imv_thumb_location"
                        android:layout_alignRight="@+id/imv_thumb_location"/>
                </RelativeLayout>
                <com.zing.zalo.feed.components.FeedMemoryPreview
                    android:id="@+id/layoutFeedMemory"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <View
                    android:id="@+id/focus_override"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:layout_width="0dp"
                    android:layout_height="0dp"/>
            </LinearLayout>
        </com.zing.zalo.social.widget.CustomScrollView>
        <RelativeLayout
            android:orientation="horizontal"
            android:id="@+id/layout_typo_location_tag_control"
            android:paddingLeft="16dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/layout_postfeed_tag_location_height"
            android:layout_marginTop="8dp"
            android:layout_above="@+id/control_button_floating">
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textSize="@dimen/f0"
                android:textColor="@color/cMtxt5"
                android:id="@+id/tv_add"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="6dp"
                android:text="@string/str_add_posf_feed"
                android:layout_centerVertical="true"/>
            <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                android:id="@+id/imv_entry_typo"
                android:background="@android:color/transparent"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginRight="6dp"
                android:src="0x7f080ef5"
                android:layout_toRightOf="@+id/tv_add"
                android:layout_centerVertical="true"/>
            <com.zing.zalo.feed.uicontrols.FeedRecyclerView
                android:id="@+id/lv_typo_graphy"
                android:visibility="gone"
                android:fadingEdgeLength="0dp"
                android:clipToPadding="false"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:listSelector="@android:color/transparent"
                android:cacheColorHint="#00000000"
                android:divider="@android:color/transparent"
                android:layout_toRightOf="@+id/imv_entry_typo"/>
        </RelativeLayout>
        <com.zing.zalo.feed.mvp.postfeed.component.ControlComposeButtonView
            android:id="@+id/control_button_floating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:layout_above="@+id/updateStatusFooterDivider"/>
        <View
            android:id="@+id/updateStatusFooterDivider"
            android:background="?attr/ItemSeparatorColor"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_above="@+id/updateStatusFooter"/>
        <RelativeLayout
            android:id="@+id/updateStatusFooter"
            android:background="?attr/PrimaryBackgroundColor"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/updateBgFunctionBar"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/function_status_bar_height">
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:id="@+id/btn_bg_attach_kb"
                    android:background="@drawable/item_background_ripple"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="@drawable/ic_postfeed_keyboard"
                    android:scaleType="center"/>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <CheckBox
                        android:gravity="center"
                        android:id="@+id/cb_post_story"
                        android:layout_width="26dp"
                        android:layout_height="26dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="6dp"
                        android:button="@drawable/mat_btn_radio_circle"
                        android:drawablePadding="3dp"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textColor="?attr/TextColor1"
                        android:gravity="center"
                        android:id="@+id/tv_post_story"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/str_post_on_story"
                        style="@style/Roboto.f13"/>
                </LinearLayout>
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:gravity="center"
                    android:id="@+id/btn_post_attach_bg_feed"
                    android:background="@drawable/item_background_ripple"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="@drawable/bg_btn_postfeed_addbg"
                    android:scaleType="center"/>
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:gravity="center"
                    android:id="@+id/btn_post_attach_bg_arrow_down"
                    android:background="@drawable/bg_btn_postfeed"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:visibility="gone"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="0x7f080d4c"
                    android:scaleType="center"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/updateStatusFunctionBar"
                android:layout_width="match_parent"
                android:layout_height="@dimen/function_status_bar_height"
                android:layout_below="@+id/updateBgFunctionBar">
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:id="@+id/btn_post_attach_sticker"
                    android:background="@drawable/item_background_ripple"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="@drawable/bg_btn_postfeed_addsticker"
                    android:scaleType="center"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="1px"
                    android:layout_weight="1"/>
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:gravity="center"
                    android:id="@+id/btn_post_attach_photo"
                    android:background="@drawable/item_background_ripple"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="@drawable/bg_btn_postfeed_addphoto"
                    android:scaleType="center"/>
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:gravity="center"
                    android:id="@+id/btn_post_attach_video"
                    android:background="@drawable/item_background_ripple"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="@drawable/bg_btn_postfeed_addvideo"
                    android:scaleType="center"/>
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:gravity="center"
                    android:id="@+id/btn_post_attach_link"
                    android:background="@drawable/item_background_ripple"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="@drawable/bg_btn_postfeed_addlink"
                    android:scaleType="center"/>
                <com.zing.zalo.uicomponents.reddot.RedDotImageButton
                    android:gravity="center"
                    android:id="@+id/btn_post_attach_location"
                    android:background="@drawable/item_background_ripple"
                    android:paddingLeft="@dimen/feed_postfeed_func_btn_padding"
                    android:paddingRight="@dimen/feed_postfeed_func_btn_padding"
                    android:layout_width="54dp"
                    android:layout_height="48dp"
                    android:src="@drawable/ic_icon_social_form_location"
                    android:scaleType="center"/>
            </LinearLayout>
            <View
                android:background="?attr/ItemSeparatorColor"
                android:layout_width="match_parent"
