<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.zing.zalo.p077ui.widget.RobotoTextView
        android:textColor="?attr/section_label"
        android:background="@drawable/bg_list_item_normal"
        android:paddingLeft="16dp"
        android:paddingTop="12dp"
        android:paddingRight="16dp"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/str_setting_utilities_title_qr_code"
        style="@style/t_small_m"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemAutoQRPhoto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:subtitle="@string/setting_privacy_sub_auto_qr_photo"
        app:title="@string/setting_privacy_auto_qr_photo"/>
    <com.zing.zalo.p077ui.settings.widget.ListItemSetting
        android:id="@+id/itemAutoParseBankCardCSC"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/str_setting_opt_in_out_bank_card_csc"/>
    <View
        android:background="?attr/SecondaryBackgroundColor"
        android:layout_width="match_parent"
        android:layout_height="8dp"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/layoutSessionJumpLink"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:background="@drawable/bg_list_item_normal"
            android:paddingRight="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.zing.zalo.p077ui.widget.RobotoTextView
                android:textColor="?attr/section_label"
                android:paddingLeft="16dp"
                android:paddingTop="12dp"
                android:paddingRight="16dp"
                android:paddingBottom="8dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/str_jump_link_setting_title"
                android:layout_weight="1"
                style="@style/t_small_m"/>
            <include layout="@layout/imgv_setting_reminder"/>
        </LinearLayout>
        <com.zing.zalo.p077ui.settings.widget.ListItemSetting
            android:id="@+id/itemJumpLinkWithZalo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:subtitle="@string/str_jump_link_open_with_zalo_des"
            app:title="@string/str_jump_link_open_with_zalo"/>
        <com.zing.zalo.p077ui.settings.widget.ListItemSetting
            android:id="@+id/itemJumpLinkWithWeb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:subtitle="@string/str_jump_link_open_with_web_des"
            app:title="@string/str_jump_link_open_with_web"/>
    </LinearLayout>
</merge>
