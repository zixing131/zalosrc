<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/personal_event_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:layout_above="@+id/attend_content"
        android:layout_alignParentTop="true">
        <LinearLayout
            android:orientation="vertical"
            android:descendantFocusability="blocksDescendants"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ViewStub
                android:id="@+id/vs_quick_action_view"
                android:layout="@layout/layout_quick_action_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:id="@+id/notify_hint_container"
                android:background="@drawable/bg_btn_type3_big"
                android:padding="8dp"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="4dp">
                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="6dp"
                    app:srcCompat="@drawable/icn_profile_form_private"/>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textSize="13dp"
                    android:textColor="?attr/TextColor2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/str_hint_notify_for_only_me"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/post_header"
                android:paddingLeft="12dp"
                android:paddingTop="12dp"
                android:paddingRight="12dp"
                android:paddingBottom="12dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.zing.zalo.p077ui.widget.AvatarImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/iv_creator_avt"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    app:srcCompat="?attr/default_avatar"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_weight="1">
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="16dp"
                        android:textColor="?attr/TextColor1"
                        android:ellipsize="end"
                        android:id="@+id/tv_creator_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxLines="1"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="11dp"
                        android:textColor="?attr/TextColor2"
                        android:id="@+id/tv_timeline"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
                <com.zing.zalo.p077ui.widget.RobotoTextView
                    android:textSize="14dp"
                    android:textColor="@color/color_press_black_text"
                    android:id="@+id/shortcut_calendar"
                    android:background="@drawable/bg_open_group_board"
                    android:paddingLeft="12dp"
                    android:paddingTop="8dp"
                    android:paddingRight="12dp"
                    android:paddingBottom="8dp"
                    android:visibility="visible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="@string/str_open_group_calendar"
                    android:drawableLeft="0x7f0809f0"
                    android:drawablePadding="6dp"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/reminder_content"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:padding="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="16dp"
                        android:textColor="?attr/TextColor1"
                        android:id="@+id/reminder_emoji"
                        android:paddingTop="2dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="16dp"
                        android:textColor="?attr/TextColor1"
                        android:id="@+id/reminder_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"/>
                </RelativeLayout>
                <View
                    android:background="?attr/ItemSeparatorColor"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/reminder_time_wrapper"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:background="@drawable/list_selector"
                    android:padding="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/reminder_time_icon"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="2dp"
                        android:scaleType="centerInside"
                        app:srcCompat="@drawable/icn_groupboard_reminder_decor_time_n"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp">
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="16dp"
                            android:textColor="?attr/TextColor1"
                            android:id="@+id/reminder_countdown_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="14dp"
                            android:textColor="?attr/TextColor2"
                            android:id="@+id/reminder_time_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="14dp"
                            android:textColor="?attr/TextColor2"
                            android:id="@+id/reminder_time_text2"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="14dp"
                            android:textColor="?attr/TextColor2"
                            android:id="@+id/reminder_lunar_time_text"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"/>
                    </LinearLayout>
                </RelativeLayout>
                <View
                    android:background="?attr/ItemSeparatorColor"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/reminder_repeat_wrapper"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:background="@drawable/list_selector"
                    android:padding="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/reminder_repeat_icon"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="2dp"
                        android:src="@drawable/icn_groupboard_reminder_decor_repeat_disable"
                        android:scaleType="centerInside"
                        android:layout_centerVertical="true"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp">
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="16dp"
                            android:textColor="?attr/TextColor1"
                            android:id="@+id/reminder_repeat_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.zing.zalo.uidrawing.ModulesView
                            android:id="@+id/reminder_repeat_monthly_note_wrapper"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </RelativeLayout>
                <View
                    android:background="?attr/ItemSeparatorColor"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/attendee_wrapper"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:background="@drawable/list_selector"
                    android:padding="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="2dp"
                        android:scaleType="centerInside"
                        android:layout_centerVertical="true"
                        app:srcCompat="@drawable/icn_groupboard_reminder_decor_accepted"/>
                    <com.zing.zalo.p077ui.widget.RobotoTextView
                        android:textSize="16dp"
                        android:textColor="?attr/TextColor1"
                        android:id="@+id/attendee_text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:includeFontPadding="false"/>
                </RelativeLayout>
                <View
                    android:background="?attr/ItemSeparatorColor"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/reminder_jump_msg_wrapper"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:background="@drawable/list_selector"
                    android:padding="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/reminder_jump_msg_icon"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="2dp"
                        android:src="@drawable/icn_groupboard_reminder_decor_message"
                        android:scaleType="centerInside"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp">
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="16dp"
                            android:textColor="?attr/TextColor1"
                            android:id="@+id/reminder_jump_msg_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/str_view_original_msg"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="14dp"
                            android:textColor="?attr/TextColor2"
                            android:ellipsize="end"
                            android:id="@+id/reminder_jump_msg_subtitle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:maxLines="1"/>
                    </LinearLayout>
                </RelativeLayout>
                <View
                    android:background="?attr/ItemSeparatorColor"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/goto_chat_csc_wrapper"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:background="@drawable/list_selector"
                    android:padding="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/goto_chat_csc_icon"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="2dp"
                        android:src="@drawable/ic_icn_groupboard_reminder_decor_group"
                        android:scaleType="centerInside"
                        android:layout_centerVertical="true"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_marginRight="16dp">
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="16dp"
                            android:textColor="?attr/TextColor1"
                            android:id="@+id/hint_open_csc"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/str_open_chat_11"
                            android:includeFontPadding="false"/>
                        <com.zing.zalo.p077ui.widget.RobotoTextView
                            android:textSize="14dp"
                            android:textColor="?attr/TextColor2"
                            android:ellipsize="end"
                            android:id="@+id/name_text_in_open_csc"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:maxLines="1"
                            android:includeFontPadding="false"/>
                    </LinearLayout>
                </RelativeLayout>
                <View
                    android:background="?attr/ItemSeparatorColor"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14dp"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <com.zing.zalo.p077ui.widget.TouchInterceptionFrameLayout
        android:id="@+id/attend_content"
        android:background="?attr/PopupBackgroundColor"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:layout_alignParentBottom="true">
        <View
            android:background="?attr/ItemSeparatorColor"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
        <RelativeLayout
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp">
            <include layout="@layout/button_attend_confirm_layout"/>
            <include layout="@layout/button_attend_confirm_loading_layout"/>
            <include layout="@layout/text_attend_confirm_layout"/>
        </RelativeLayout>
    </com.zing.zalo.p077ui.widget.TouchInterceptionFrameLayout>
</RelativeLayout>
