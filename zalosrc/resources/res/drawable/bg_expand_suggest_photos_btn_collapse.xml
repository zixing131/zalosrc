<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:startColor="#aa000000"
                android:endColor="#00000000"
                android:angle="0"
                android:type="radial"
                android:centerX="50%"
                android:centerY="50%"
                android:gradientRadius="55%p"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_icon_expand"
        android:left="0.5dp"
        android:top="@dimen/multi_photo_suggest_expand_btn_collapse_shadow_top"
        android:right="0.5dp"
        android:bottom="@dimen/multi_photo_suggest_expand_btn_collapse_shadow_bottom"/>
</layer-list>
