<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:state_checked="false"
        android:drawable="@drawable/switch_thumb_off_disable"/>
    <item
        android:state_enabled="false"
        android:state_checked="true"
        android:drawable="@drawable/switch_thumb_on_disable"/>
    <item
        android:state_enabled="true"
        android:state_checked="true"
        android:drawable="@drawable/switch_thumb_on_enable"/>
    <item
        android:state_enabled="true"
        android:state_checked="false"
        android:drawable="@drawable/switch_thumb_off_enable"/>
</selector>
