<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="true"
        android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="?attr/switch_handle"/>
            <size
                android:height="24dp"
                android:width="24dp"/>
        </shape>
    </item>
    <item
        android:state_enabled="true"
        android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="?attr/switch_handle"/>
            <size
                android:height="24dp"
                android:width="24dp"/>
        </shape>
    </item>
    <item
        android:state_enabled="false"
        android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="?attr/switch_handle_disabled"/>
            <size
                android:height="24dp"
                android:width="24dp"/>
        </shape>
    </item>
    <item
        android:state_enabled="false"
        android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="?attr/switch_handle_disabled"/>
            <size
                android:height="24dp"
                android:width="24dp"/>
        </shape>
    </item>
</selector>
