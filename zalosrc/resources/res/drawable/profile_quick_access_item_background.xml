<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/stroke">
        <shape>
            <stroke
                android:width="1.5dp"
                android:color="?attr/ProfileQuickAccessShadowColor"/>
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="1.5dp"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
    <item android:id="@+id/background">
        <shape>
            <solid android:color="?attr/ProfileQuickAccessBackgroundColor"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
</layer-list>
