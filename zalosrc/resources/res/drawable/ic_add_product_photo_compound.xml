<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/ic_add_product_photo"/>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="8dp"/>
            <size
                android:height="96dp"
                android:width="96dp"/>
            <stroke
                android:width="1dp"
                android:color="?attr/border_02"
                android:dashWidth="5dp"
                android:dashGap="2dp"/>
        </shape>
    </item>
</layer-list>
