<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners
                android:topLeftRadius="8dp"
                android:bottomRightRadius="8dp"/>
            <solid android:color="?attr/bgCSC_Background"/>
        </shape>
    </item>
    <item
        android:left="2dp"
        android:top="2dp">
        <shape>
            <corners
                android:topLeftRadius="8dp"
                android:bottomRightRadius="8dp"/>
            <solid android:color="?attr/layer_container"/>
        </shape>
    </item>
</layer-list>
